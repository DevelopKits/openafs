srcdir=@srcdir@
include @TOP_OBJDIR@/src/config/Makefile.config


LIBS=${TOP_LIBDIR}/libcmd.a ${TOP_LIBDIR}/libcom_err.a ${TOP_LIBDIR}/util.a

INCDIRS=  -I${SRCDIR}/include -I${SRCDIR}/include/afs -I..

CFLAGS = ${OPTIMIZE} ${INCDIRS} ${XCFLAGS} ${ARCHFLAGS}

all: test
test tests: ctest dtest itest

itest: itest.o 
	cc ${CFLAGS} -o itest itest.o ${LIBS} ${XLIBS}

ctest: ctest.o
	cc ${CFLAGS} -o ctest ctest.o ${LIBS} ${XLIBS}

dtest: dtest.o
	cc ${CFLAGS} -o dtest dtest.o ${LIBS} ${XLIBS}

system: test

clean:
	$(RM) -f  *.a *.o ctest dtest itest core

install:
dest:

CCRULE=${CC} ${CFLAGS} -c $? -o $@

PUBLICHEADERS= \
	agent_client.h \
	console_registration.h \
	init.h \
	plugin.h \
	plugin_types.h \
	probe_client.h \
	proc_client.h \
	proc_client_impl.h \
	proc_client_impl_types.h \
	proc_client_types.h \
	trap.h \
	trap_impl.h \
	trap_queue.h \
	trap_types.h \
	$(NULL)

DEPINSTALL_HDR_DIR=${TOP_INCDIR}/osi/trace/console
INSTALL_HDR_DIR=${DESTDIR}${includedir}/osi/trace/console
DEST_HDR_DIR=${DEST}/include/osi/trace/console
include @TOP_OBJDIR@/src/config/Makefile.public-header

OBJS=

OBJECTS=${OBJS}


all:

depinstall: \
	depinstall_hdrs

#
# Installation targets
#
install: \
	install_hdrs

dest: \
	dest_hdrs

#
# Build targets
#


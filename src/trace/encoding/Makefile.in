srcdir=@srcdir@
include @TOP_OBJDIR@/src/config/Makefile.config
HELPER_SPLINT=@HELPER_SPLINT@

CFLAGS = ${COMMON_CFLAGS} -D${SYS_NAME} ${FSINCLUDES} ${XCFLAGS} ${ARCHFLAGS} ${OSI_CFLAGS}

CCRULE=${CC} ${CFLAGS} -c $? -o $@

PUBLICHEADERS= \
	demux.h \
	demux_impl.h \
	demux_types.h \
	encoding.h \
	handle.h \
	handle_impl.h \
	handle_impl_types.h \
	handle_types.h \
	init.h \
	init_impl.h \
	trap_types.h \
	$(NULL)

DEPINSTALL_HDR_DIR=${TOP_INCDIR}/osi/trace/encoding

DEST_HDR_DIR=${DEST}/include/osi/trace/encoding
include @TOP_OBJDIR@/src/config/Makefile.public-header

RECURSE_DIRS = \
	ascii \
	$(NULL)

include @TOP_OBJDIR@/src/config/Makefile.recurse

OBJS=

OBJECTS=${OBJS}


all: \
	build_recurse \
	$(NULL)

depinstall: \
	depinstall_hdrs \
	depinstall_recurse \
	$(NULL)

#
# Installation targets
#
install: \
	install_hdrs \
	install_recurse \
	$(NULL)

dest: \
	dest_hdrs \
	dest_recurse \
	$(NULL)


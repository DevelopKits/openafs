depinstall_hdrs: ${PUBLICHEADERS}
	for hdr in x ${PUBLICHEADERS} ; \
	do \
		if [ "$${hdr}" != "x" ] ; then \
			${INSTALL_DATA} $$hdr ${DEPINSTALL_HDR_DIR}/$$hdr ; \
		fi ; \
	done

install_hdrs: ${PUBLICHEADERS}
	for hdr in x ${PUBLICHEADERS} ; \
	do \
		if [ "$${hdr}" != "x" ] ; then \
			${INSTALL_DATA} $$hdr ${INSTALL_HDR_DIR}/$$hdr ; \
		fi ; \
	done

dest_hdrs: ${PUBLICHEADERS}
	for hdr in x ${PUBLICHEADERS} ; \
	do \
		if [ "$${hdr}" != "x" ] ; then \
			${INSTALL_DATA} $$hdr ${DEST_HDR_DIR}/$$hdr ; \
		fi ; \
	done


srcdir=@srcdir@
include @TOP_OBJDIR@/src/config/Makefile.config


INCDIRS= -I${TOP_OBJDIR}/src/config -I${TOP_INCDIR} -I..
LIBS=${TOP_LIBDIR}/libsys.a ${TOP_LIBDIR}/libubik.a \
	${TOP_LIBDIR}/libvldb.a ${TOP_LIBDIR}/libauth.a \
	${TOP_LIBDIR}/librxkad.a ${TOP_LIBDIR}/libdes.a \
	${TOP_LIBDIR}/libcom_err.a ${TOP_LIBDIR}/libcmd.a \
	${TOP_LIBDIR}/librx.a ${TOP_LIBDIR}/liblwp.a \
	${TOP_LIBDIR}/util.a

CFLAGS = ${OPTIMIZE} ${INCDIRS} ${XCFLAGS} ${ARCHFLAGS}

all test:  fulltest owntest idtest getinitparams

install:
dest:

fulltest: fulltest.o
	$(CC) $(CFLAGS) -o $@ $@.o $(LIBS)
owntest: owntest.o
	$(CC) $(CFLAGS) -o $@ $@.o $(LIBS)
idtest: idtest.o
	$(CC) $(CFLAGS) -o $@ $@.o $(LIBS)
getinitparams: getinitparams.o
	$(CC) $(CFLAGS) -o $@ $@.o $(LIBS)

clean:
	$(RM) -f *.o fulltest owntest idtest getinitparams


srcdir=@srcdir@
include @TOP_OBJDIR@/src/config/Makefile.config



INCDIRS=-I${TOP_INCDIR} -I.. -I${TOP_INCDIR}/afs
LIBS=${TOP_LIBDIR}/libauth.a ${TOP_LIBDIR}/librxkad.a \
	${TOP_LIBDIR}/libdes.a ${TOP_LIBDIR}/libsys.a \
	${TOP_LIBDIR}/librx.a ${TOP_LIBDIR}/liblwp.a \
	${TOP_LIBDIR}/libcmd.a ${TOP_LIBDIR}/util.a

CFLAGS = ${OPTIMIZE} ${INCDIRS} ${LIBS} ${XCFLAGS} ${ARCHFLAGS}

all: testlog gettoktest

install:  

#
# Build targets
#
testlog: testlog.o 
	${CC} -o testlog testlog.o ${CFLAGS} ${LIBS} ${XLIBS}

gettoktest: gettoktest.o 
	${CC} -o gettoktest gettoktest.o ${CFLAGS} ${LIBS} ${XLIBS}

#
# Misc. targets
# 
clean:	
	$(RM) -f *.o testlog gettoktest core

dest:  

